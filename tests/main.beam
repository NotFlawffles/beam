let pub EXIT_SUCCESS: int = 0;
let pub EXIT_FAILURE: int = 1;

let pub main(argc: int, argv: char**): int {
    let argc_plus_one: int = argc + 1;
    let mut index: int = -1;
    let mut starts_with_n: bool = false;

    if argc_plus_one < 2 {
	return EXIT_FAILURE;
    }

    do {
	if starts_with_n {
	    break; // break before finishing iteration reduce delay
	}

	starts_with_n = *(*(argv + 1)) & 'n';

	index += 1;
    } while index < argc_plus_one;

    return EXIT_SUCCESS;
}
